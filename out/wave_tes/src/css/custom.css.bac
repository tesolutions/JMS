/* Footer */
.footer {
	color: #181715;
	background-color: #f5f5f5;
}
.footertop {
	background: #e9e9e9;
	padding-top: 6px;
	padding-bottom: 6px;
}
.footer-box-title {
	font-size: 16px;
}
.footer-box-content {
	font-size: 13px;
	line-height: 16px;
}
