	list-style-type: none;
}
.deliveryInfo {
	font-size: 12px;
	line-height: 12px;
}
.social-links {
    text-align:center
}
.social-links-list {
    padding-left: 0;
    list-style:none;
	margin-bottom: 0;
	margin-top: 1rem;
}
.social-links-item {
    margin: 0 10px;
    font-size: 14px;
    display: inline-block;
    padding-left: 5px;
    padding-right:5px
}
